:root {
    flex: 1 1 0;
}


.table {
    flex: 1 1 0;
}

.cell {
    flex-direction: row;
    align-items: center;
    align-content: center;
    flex-grow: 1;
    height: 25px;
}

.icon {
    flex: 0 0 0;
    flex-direction: row;
    background-color: rgba(0, 0, 0, 0);
}

.label {
    align-self: center;

}

.no-data-label{
    flex: 1 1 0;
    -unity-text-align: middle-center;
    overflow: hidden;
}

.unity-multi-column-header__column--sortable {
    flex-direction: row-reverse;
}

#unity-multi-column-header-column-title{
    overflow: hidden;
    text-overflow: ellipsis;
}

<ui:UXML xmlns:ui="UnityEngine.UIElements" noNamespaceSchemaLocation="../../../../../../UIElementsSchema/UIElements.xsd"
         editor-extension-mode="False">
    <Style src="./SpriteAtlasRawData.uss"/>
    <Style
        src="project://database/Packages/com.unity.2d.tooling/Editor/Insider/SpriteAtlas/SpriteAtlasIssueReport/UIElement/StyleSheet/Icon.uss"/>
    <ui:Template name="cell-template"
                 src="project://database/Packages/com.unity.2d.tooling/Editor/Insider/SpriteAtlas/SpriteAtlasIssueReport/UIElement/CellLabelWithIcon/CellLabelWithIcon.uxml"/>
    <ui:MultiColumnTreeView name="Table" show-alternating-row-backgrounds="ContentOnly" horizontal-scrolling="true"
                            sorting-mode="Default" class="table">
        <ui:Columns primary-column-name="Name" resize-preview="false">
            <ui:Column name="Name" title="Name" cell-template="cell-template" binding-path="name" width="80px"/>
            <ui:Column name="Sprites" title="Sprites" cell-template="cell-template" binding-path="sprites"
                       width="100px"/>
            <ui:Column name="Total Memory" title="Total Memory" cell-template="cell-template" binding-path="totalMemory"
                       width="80px"/>
            <ui:Column name="Unused Memory" title="Unused Memory" cell-template="cell-template"
                       binding-path="unusedMemory" width="80px"/>
            <ui:Column name="Area Usage %" title="Usage" cell-template="cell-template" binding-path="usage"
                       width="80px"/>
            <ui:Column name="Total Area" title="Total Area" cell-template="cell-template" binding-path="totalArea"
                       width="80px"/>
            <ui:Column name="Used Area" title="Used Area" cell-template="cell-template" binding-path="usedArea"
                       width="80px"/>
            <ui:Column name="Width" title="Width" cell-template="cell-template" binding-path="width" width="80px"/>
            <ui:Column name="Height" title="Height" cell-template="cell-template" binding-path="height" width="80px"/>
            <ui:Column name="Packing Mode" title="Packing Mode" cell-template="cell-template" binding-path="packingMode"
                       width="80px"/>
            <ui:Column name="Texture Format" title="Texture Format" cell-template="cell-template"
                       binding-path="textureFormat" width="80px"/>
        </ui:Columns>
    </ui:MultiColumnTreeView>
    <ui:Label name="NoDataLabel" class="no-data-label" text="No issues found"/>
</ui:UXML>

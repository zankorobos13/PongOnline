:root{

}

#icon{
    flex: 0 0 0;
    min-width: 16px;
    min-height: 16px;
    flex-direction: row;
    background-color: rgba(0, 0, 0, 0);
}

.sprite-icon {
    background-image: resource('Sprite Icon');
}

.dark .sprite-icon {
    background-image: resource('d_Sprite Icon');
}

.dark .spriteatlas-icon {
    background-image: resource('SpriteAtlasAsset Icon');
}

.dark .spriteatlas-icon {
    background-image: resource('d_SpriteAtlasAsset Icon');
}

#cell {
    flex: 1 1 0;
    flex-direction: row;
    align-items: center;
    align-content: center;
    height: 25px;
    margin-left: 5px;
}

#countLabel{
    height: 25px;
    margin-left: 5px;
    -unity-text-align: middle-left;
    align-self: flex-start;
}
